{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Trace",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Microsoft.EntityFrameworkCore.Database.Command": "Information"
    },
    "NLog": "data source=data-source;Initial Catalog=UserDb;user id=user-id;password=password"
  },
  "CurrentDomain": "http://localhost/TourV2.API",
  "connectionStrings": {
    "UserDbConnectionString": "data source=data-source;Initial Catalog=UserDb;user id=user-id;password=password"
  },
  "AllowedHosts": "*",
  "JwtSettings": {
    "key": "This*Is&A!Long)Key(For%Creating@A$SymmetricKey",
    "issuer": "http://localhost:5000",
    "audience": "DocumentUsers",
    "minutesToExpiration": "720"
  },
  "UserProfilePath": "Users"
}
